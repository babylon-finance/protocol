{
  "name": "@babylon-finance/protocol",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^6.0.5",
    "@typechain/hardhat": "^1.0.1",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "fs-extra": "^9.1.0",
    "hardhat-contract-sizer": "^2.0.2",
    "hardhat-deploy-ethers": "^0.3.0-beta.7",
    "hardhat-docgen": "^1.0.3",
    "hardhat-gas-reporter": "^1.0.4",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.6",
    "solhint": "^3.3.4",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.3.0",
    "@openzeppelin/contracts-upgradeable": "^3.4.1",
    "@uniswap/lib": "^4.0.1-alpha",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "https://github.com/hoytech/uniswap-v2-periphery.git#e8c36e5ad5178341cd67f8b0fdb983073428fb6b",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "ethereum-waffle": "^3.0.2",
    "ethers": "^5.0.7",
    "hardhat": "^2.0.5",
    "node-watch": "^0.6.4",
    "superagent": "^6.1.0"
  },
  "scripts": {
    "clean": "rm -rf cache/* && rm -rf artifacts/* && rm -rf ./publish/contracts/*",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint:contracts": "solhint contracts/*.sol",
    "compile": "hardhat compile --no-typechain",
    "typechain": "hardhat compile",
    "publish": "hardhat run scripts/publish.js",
    "deploy": "hardhat run scripts/deploy.js --network localhost && hardhat run scripts/publish.js",
    "deploy-mainnet": "hardhat run scripts/deploy.js --network mainnet && hardhat run scripts/publish.js",
    "watch": "node scripts/watch.js",
    "chain": "hardhat node",
    "accounts": "hardhat accounts",
    "account": "hardhat account",
    "generate": "hardhat generate",
    "balance": "hardhat balance",
    "send": "hardhat send",
    "test": "hardhat test --network hardhat",
    "gas-report": "env REPORT_GAS=1 yarn test",
    "estimate-size": "yarn run hardhat size-contracts",
    "coverage": "hardhat coverage"
  }
}
